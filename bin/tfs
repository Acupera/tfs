#!/usr/bin/env node

var program = require('commander');

program
  .usage('<command>')
  .command('changeset [options]', 'Changesets information and editing.')
  .command('checkin [options]', 'Commit pending changes.')
  .command('get [options]', 'Retrieve a read-only copy of a file.')
  .command('undo [options]', 'Remove pending changes from a workspace.')
  .command('workspaces [options]', 'Display information about workspaces.')
;

program.parse(process.argv);
