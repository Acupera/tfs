#!/usr/bin/env node

var hasError      = false,
    program       = require('commander'),
    tfsWorkspaces = require('../lib/tfs/workspaces');

program
  .usage('[options]')
  .option('-V, --verbose', 'Verbose mode.')
  .parse(process.argv);

var options = {
  verbose: program.verbose
};

tfsWorkspaces(program.args, options);
